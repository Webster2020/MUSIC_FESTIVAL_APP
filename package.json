{
  "name": "my-awesome-package",
  "version": "1.0.0",
  "engines": {
    "node": "14.16.0"
  },
  "description": "The best package you will ever find.",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/yarnpkg/example-yarn-package",
    "type": "git"
  },
  "scripts": {
    "dev": "npx nodemon server.js",
    "build": "cd client && yarn install && yarn build",
    "test": "set NODE_ENV=production && mocha \"./{,!(node_modules|client)/**/}*.test.js\"",
    "test:watch": "onchange \"./**/*.js\" -i -k -- npm run test"
  },
  "author": "Yarn Contributor",
  "license": "MIT",
  "dependencies": {
    "chai": "4.2.0",
    "chai-http": "4.3.0",
    "cors": "^2.8.5",
    "express": "4.17.1",
    "express-handlebars": "3.1.0",
    "helmet": "3.21.1",
    "mocha": "6.2.1",
    "mongo-sanitize": "1.0.1",
    "mongodb": "3.3.2",
    "mongoose": "5.7.1",
    "onchange": "6.1.0",
    "snyk": "1.235.0",
    "socket.io": "2.2.0",
    "uuid": "^8.3.2"
  }
}
